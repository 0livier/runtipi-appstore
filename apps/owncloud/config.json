{
  "$schema": "../schema.json",
  "name": "Owncloud",
  "port": 8151,
  "available": true,
  "exposable": true,
  "id": "owncloud",
  "tipi_version": 1,
  "version": "10.11.0",
  "categories": ["data"],
  "description": "ownCloud gives you freedom and control over your own data. A personal cloud which runs on your own server.  ",
  "short_desc": "A personal cloud which runs on your own server. ",
  "author": "https://github.com/owncloud",
  "source": "https://github.com/owncloud/core",
  "website": "https://owncloud.com/",
  "form_fields": [
    {
      "type": "random",
      "label": "MYSQL_PASSWORD",
      "min": 32,
      "env_variable": "MYSQL_PASSWORD"
    },
    {
      "type": "text",
      "label": "Owncloud Admin Username",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "ADMIN_USERNAME"
    },
    {
      "type": "password",
      "label": "Owncloud Admin Password",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "OWNCLOUD_ADMIN_PASSWORD"
    }
  ]
}
